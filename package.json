    "vite": "^4.1.0"
  },
  "scripts": {
    "build": "npm run check && REACT_APP_GIT_SHA=`git rev-parse --short HEAD` react-app-rewired build",
    "build:ci": "npm run ci && REACT_APP_GIT_SHA=`git rev-parse --short HEAD` react-app-rewired build",
    "check": "npm run lint && npm run test:ci",
    "ci": "npm run lint && npm run commitlint-travis && npm run test:ci",
    "commit": "./node_modules/cz-customizable/standalone.js",
    "deploy": "gh-pages -d build",
    "gh-pages": "CI=true npm run build && gh-pages -d build",
    "lint": "eslint --ext .js,.jsx src/",
    "commitlint-travis": "commitlint-travis",
    "predeploy": "npm run build",
    "release": "standard-version",
    "start": "REACT_APP_GIT_SHA=`git rev-parse --short HEAD` HTTPS=true react-app-rewired start",
    "test": "REACT_APP_GIT_SHA=`git rev-parse --short HEAD` react-app-rewired test",
    "test:ci": "CI=true react-app-rewired test --coverage",
    "dev": "VITE_GIT_SHA=`git rev-parse --short HEAD` vite",
    "build:vite": "vite build",
    "preview": "vite preview"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "bin": {},
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
